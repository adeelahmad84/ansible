configure
    port 1/1/1
        description "[no-mon,core,ip=,rd=pr2.thlon:te0-2-0-6-2,cid=1000/LNTW-OSTF/10GLAN/OVN14,iv-s=bb]"
        network
            egress
                pool
                    slope-policy "Sky slope-policy"
                exit
            exit
        exit
        ethernet
            mtu 8222
            egress-scheduler-policy "P-CORE-EGRESS-10G"
            network
                queue-policy "network-queue-10Gig-imm"
            exit
            hold-time up 5
            efm-oam
                transmit-interval 1 multiplier 3
				hold-time 5
                no shutdown
            exit
            lldp
                dest-mac nearest-bridge
                    admin-status tx-rx
                    tx-tlvs port-desc sys-name sys-desc sys-cap
                    tx-mgmt-address system
                exit
            exit
        exit
        no shutdown
    exit
