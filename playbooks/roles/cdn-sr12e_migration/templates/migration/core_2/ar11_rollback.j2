exit all
configure
{% for key, value in core_2|dictsort %}
    port {{ value.remote_port }}
        description "[no-mon,core,ip=,rd={{ core_2_hostname }}:{{ value.port|replace("/","-") }},cid={{ value.cid }},iv-s=vcdn_sc]"
        shutdown
    exit
{% endfor %}
    lag 2
        description "[no-mon,core,ip=,rd={{ core_2_hostname }}:be{{ system_last_octet|int + 256 }},iv-s=vcdn_sc]"
        shutdown
exit all