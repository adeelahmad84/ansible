{% extends "4500_common.j2" %}
{% block bbpe3 %}
{% for item, value in bbpe.3.ports|dictsort %}
interface TenGigabitEthernet{{ value.remote_port }}
 description [core,rd={{ bbpe.3.hostname }}:{{ value.port|replace("/","-") }},cid=LOCAL,iv-s=cdn_pop] UPLINK_{{ loop.index }} TO {{ bbpe.3.hostname|upper }}
 no switchport
 mtu 8226
 no ip address
 no ip redirects
 channel-protocol lacp
 channel-group {{ bbpe.3.channel }} mode active
 no shutdown
!
{% endfor %}
{% endblock %}
