
{% for devices, bbpe in bbpe|dictsort %}
########{{ bbpe.hostname|upper }}################
exit all
configure
{% for key, cdn in services|dictsort %}
    router
        no static-route {{ cdn.ipv6 | ipaddr('0') }} next-hop {{ bbpe.ipv6_p2p | ipaddr('1')|regex_replace('/.*', '') }}
        no static-route {{ cdn.ipv4 | ipaddr('0') }} next-hop {{ bbpe.ipv4_p2p | ipaddr('1')|regex_replace('/.*', '') }} tag {{ bbpe.tag }}
        policy-options
	    begin
            prefix-list "POPCDN-IPV6-{{ prefix_name|upper }}-LAN"
                no prefix {{ cdn.ipv6 }}
	    exit
{% if cdn.svc_id == 0 %}
            no community "cdn-{{ site_id }}-{{ prefix_name|lower|replace("ggc", "google") }}"
            policy-statement "EXPORT-TO-IBGP"
                no entry {{ v4_entry }}
	    exit
            policy-statement "EXPORT-TO-IBGP6"
                no entry {{ v6_entry }}
	    exit
{% endif %}
	    commit
exit all
{% endfor %}
########{{ bbpe.hostname|upper }}################
{% endfor %}